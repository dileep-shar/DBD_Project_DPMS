    "nodemon": "^2.0.20",
    "react-data-table-component": "^7.5.3",
    "react-dropbox-chooser": "^0.0.5",
    "react-dropdown": "^1.11.0",
    "react-firebase-hooks": "^5.1.1",
    "react-google-login": "^5.2.2",
    "react-router-dom": "^6.4.4",
    "styled-components": "^5.3.6"
  },
  "devDependencies": {
    "autoprefixer": "^10.4.13",
    "postcss": "^8.4.19",
    "tailwindcss": "^3.2.4"
  }
